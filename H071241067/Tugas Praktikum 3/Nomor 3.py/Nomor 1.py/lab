def hitung_frekuensi_karakter(kalimat):
  """
  Fungsi ini menghitung frekuensi kemunculan setiap karakter dalam sebuah kalimat.

  Args:
    kalimat: Kalimat yang akan dihitung frekuensi karakternya.

  Returns:
    Dictionary yang berisi karakter sebagai key dan jumlah kemunculannya sebagai value.
  """

  frekuensi = {}
  for karakter in kalimat:
    if karakter in frekuensi:
      frekuensi[karakter] += 1
    else:
      frekuensi[karakter] = 1
  return frekuensi

# Contoh penggunaan
kalimat = "Ini adalah contoh kalimat untuk dihitung frekuensi karakternya"
hasil = hitung_frekuensi_karakter(kalimat)

print(hasil)